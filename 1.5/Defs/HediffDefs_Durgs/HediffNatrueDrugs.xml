<?xml version="1.0" encoding="UTF-8"?>
<Defs>
    <!-- 储灵草籽的buff-->
    <HediffDef>
		<defName>LRE_SaveEnergyGrassSeedHediff</defName>
		<label>Save Energy Grass Seed Hediff</label>
		<hediffClass>HediffWithComps</hediffClass>
		<description>Save Energy Grass Seed Hediff.</description>
		<scenarioCanAdd>false</scenarioCanAdd>
		<isBad>false</isBad>
		<comps>
            <!-- 修炼进度-->
			<li Class = "RimLmmortals_LHXExtend.LRE_Properties.HediffCompProperties_AddInExpStored">
				<expValue>100</expValue>
			</li>
            <li Class = "RimLmmortals_LHXExtend.LRE_Properties.HediffCompProperties_AddHediff">
                <hediffDef>LRE_EnergyGainEffHediff</hediffDef>
                <severity>1</severity>
                <getMax>true</getMax>
            </li>
            <li Class="HediffCompProperties_Disappears"/>
		</comps>
	</HediffDef>

</Defs>
