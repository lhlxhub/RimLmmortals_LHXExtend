<?xml version="1.0" encoding="UTF-8"?>
<Defs>
    <!-- 天生储灵丹的buff-->
    <HediffDef>
		<defName>LRE_SaveEnergyGrassSeedHediff</defName>
		<label>Save Energy Grass Seed Hediff</label>
		<hediffClass>HediffWithComps</hediffClass>
		<description>Save Energy Grass Seed Hediff.</description>
		<scenarioCanAdd>false</scenarioCanAdd>
		<isBad>false</isBad>
		<comps>
            <!-- 修炼进度-->
			<li Class = "RimLmmortals_LHXExtend.HediffComps.HediffCompProperties_AddInExpStored">
				<fValue>100</fValue>
			</li>
            <li Class = "RimLmmortals_LHXExtend.HediffComps.HediffComp_AddHediff">
                <hediffDef>LRE_EnergyGainEffHediff</hediffDef>
                <severity>1</severity>
                <getMax>true</getMax>
            </li>
            <li Class="HediffCompProperties_Disappears"/>
		</comps>
	</HediffDef>

</Defs>
